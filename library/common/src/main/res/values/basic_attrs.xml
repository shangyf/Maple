<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--标题-->
    <attr name="title" format="string|reference" />
    <attr name="titleColor" format="color" />
    <attr name="titleSize" format="dimension" />
    <attr name="bgTitle" format="color" />
    <!--右边的按钮-->
    <attr name="rightTitle" format="string|reference" />
    <attr name="rightColor" format="color" />
    <attr name="rightSize" format="dimension" />
    <!--返回按钮-->
    <attr name="backArrow" format="reference" />

    <!--标题-->
    <declare-styleable name="TitleBar">
        <attr name="title" />
        <attr name="titleColor" />
        <attr name="titleSize" />
        <attr name="rightTitle" />
        <attr name="rightColor" />
        <attr name="rightSize" />
        <attr name="backArrow" />
        <attr name="bgTitle" />
    </declare-styleable>

    <!--设置-->
    <declare-styleable name="SettingView">

    </declare-styleable>
</resources>