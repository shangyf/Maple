<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--标题-->
    <attr name="title" format="string|reference" />
    <attr name="titleColor" format="color|reference" />
    <!--integer-->
    <attr name="titleSize" format="integer" />
    <attr name="bgTitle" format="color" />
    <!--右边的按钮-->
    <attr name="rightTitle" format="string|reference" />
    <attr name="rightColor" format="color|reference" />
    <attr name="rightSize" format="integer" />
    <!--返回按钮-->
    <attr name="backArrow" format="reference" />
    <!--是否显示左边-->
    <attr name="isShowLeft" format="boolean" />
    <!--是否显示右边-->
    <attr name="isShowRight" format="boolean" />

    <!--标题-->
    <declare-styleable name="TitleBar">
        <attr name="title" />
        <attr name="titleColor" />
        <attr name="titleSize" />
        <attr name="rightTitle" />
        <attr name="rightColor" />
        <attr name="rightSize" />
        <attr name="backArrow" />
        <attr name="bgTitle" />
        <attr name="isShowLeft"/>
        <attr name="isShowRight"/>
    </declare-styleable>

    <!--设置-->
    <declare-styleable name="SettingView">

    </declare-styleable>
</resources>